                    "new_pool": "wotr:rift/anomaly/halfway/3",
                    "chance": 0.15
                },
                "wotr:rift/poi/inwall/3": {
                    "new_pool": "wotr:rift/anomaly/inwall/3",
                    "chance": 0.15
                }
